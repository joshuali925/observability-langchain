{
  "name": "olly-test",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "test": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest --config ./configs/jest.config.ts --runInBand",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node src/index.ts",
    "postinstall": "find ./packages -name requirements.txt -type f -execdir sh -c '[ ! -d venv ] && python3 -m venv venv; source venv/bin/activate; pip install -r {}' \\;"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-license-header": "^0.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest-html-reporters": "^3.1.7",
    "lint-staged": "^15.0.2",
    "nanoid": "^3.3.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@opensearch-project/opensearch": "^2.4.0",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "promptfoo": "^0.31.2"
  }
}
